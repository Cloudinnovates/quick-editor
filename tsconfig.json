  },
  "files": [
    "./typings/tsd.d.ts",
    "./quick-editor.ts",
    "./src/editable.ts",
    "./src/editor-metadata.ts",
    "./src/quick-editor.component.ts",
    "./src/quick-editor-element.ts"
  ],
  "exclude": [
    "node_modules"
  ]
